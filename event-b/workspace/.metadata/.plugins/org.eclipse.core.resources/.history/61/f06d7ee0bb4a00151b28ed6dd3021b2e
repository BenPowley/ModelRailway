<org.eventb.core.extendsContext name="'" org.eventb.core.target="Railway"/>
<org.eventb.core.constant name="(" org.eventb.core.identifier="nextRoute"/>
<org.eventb.core.axiom name=")" org.eventb.core.label="axm1" org.eventb.core.predicate="nextRoute ∈ (routes → ((1‥max1) → track))"/>
<org.eventb.core.axiom name="*" org.eventb.core.label="axm2" org.eventb.core.predicate="∀r·∀x· r∈routes ∧  x∈dom(nextRoute(r))∧ x+1 ∈ dom(r) ⇒ nextRoute(r)(x) = r(x+1)"/>
<org.eventb.core.axiom name="," org.eventb.core.label="axm4" org.eventb.core.predicate="∀r ·r∈ routes ⇒ ran(nextRoute(r)) ⊆ ran(r)"/>
<org.eventb.core.axiom name="+" org.eventb.core.label="axm3" org.eventb.core.predicate="∀r· r∈ routes ⇒ ran(nextRoute(r)) ⊆ track"/>
